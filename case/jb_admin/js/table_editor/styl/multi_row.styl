.com-table-multi-row
  padding-top 2px
  .myrow:not(:last-child):after
    content ''
    display block
    width 100%
    height 0
    border-bottom 1px solid #e2e2e2
    left: 0;
    position: absolute

  div.myrow
    padding 5px 0
    white-space nowrap
    text-overflow: ellipsis;
    overflow : hidden;

    span
      display inline-block
      height 1em
