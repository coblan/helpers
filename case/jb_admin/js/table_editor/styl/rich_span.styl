.com-table-grid
  .middle-col
    text-align: center !important
  .btn-like-col
    .com-table-rich-span
      border-radius:3px;
      font-size:80%;
      display:inline-block;
      height: 16px;
      padding: 0 5px;
      line-height: 16px
      min-width 40px
      &.warning
        color:white;
        background-color:red
      &.success
        color:white;
        background-color:green
      &.primary
        color:white
        background-color #499cff