.com-headbar-sys-link
  height 100%
  padding 15px
  line-height: 20px;
  float right
  color #dcdcdc
  &.active
    color #fdf7ff
    background-color #980000
    &:hover
      background-color #bd0000

  &:hover
    background: rgba(0,0,0,0.1);
    color: #f6f6f6
  &.link:hover
    cursor pointer



.com-headbar-space
  display inline-block
  float right
  height 10px